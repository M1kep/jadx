<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE RSyntaxTheme SYSTEM "theme.dtd">


<RSyntaxTheme version="1.0">

    <!--  General editor colors. -->
    <background color="2B2B2B"/>
    <caret color="A9B7C6"/>
    <selection useFG="true" bg="214283" fg="A9B7C6" roundedEdges="false"/>
    <currentLineHighlight color="323232" fade="false"/>
    <marginLine fg="394448"/>
    <markAllHighlight color="6b8189"/>
    <markOccurrencesHighlight color="5b7179" border="false"/>
    <matchedBracket fg="6A8088" bg="6b8189" highlightBoth="false" animate="true"/>
    <hyperlinks fg="a082bd"/>
    <secondaryLanguages>
        <language index="1" bg="9876AA"/>
        <language index="2" bg="223322"/>
        <language index="3" bg="332222"/>
    </secondaryLanguages>

    <!-- Gutter styling. -->
    <gutterBorder color="81969A"/>
    <lineNumbers fg="A9B7C6"/>
    <foldIndicator fg="6A8088" iconBg="2f383c"/>
    <iconRowHeader activeLineRange="3399ff"/>

    <!-- Syntax tokens. -->
    <tokenStyles>
        <style token="IDENTIFIER" fg="A9B7C6"/><!-- done -->
        <style token="RESERVED_WORD" fg="CB772F" bold="true"/><!-- done -->
        <style token="RESERVED_WORD_2" fg="CB772F" bold="true"/><!-- done -->
        <style token="ANNOTATION" fg="A9B7C6"/><!-- done -->
        <style token="COMMENT_DOCUMENTATION" fg="619647"/><!-- done -->
        <style token="COMMENT_EOL" fg="808080"/><!-- done -->
        <style token="COMMENT_MULTILINE" fg="619647"/><!-- done -->
        <style token="COMMENT_KEYWORD" fg="619647"/><!-- done -->
        <style token="COMMENT_MARKUP" fg="619647"/><!-- done -->
        <style token="FUNCTION" fg="A9B7C6"/><!-- done -->
        <style token="DATA_TYPE" fg="A9B7C6" bold="true"/>
        <style token="LITERAL_BOOLEAN" fg="6897BB" bold="true"/><!-- done -->
        <style token="LITERAL_NUMBER_DECIMAL_INT" fg="6897BB"/><!-- done -->
        <style token="LITERAL_NUMBER_FLOAT" fg="6897BB"/><!-- done -->
        <style token="LITERAL_NUMBER_HEXADECIMAL" fg="6897BB"/><!-- done -->
        <style token="LITERAL_STRING_DOUBLE_QUOTE" fg="A5C25C"/><!-- done -->
        <style token="LITERAL_CHAR" fg="A5C25C"/><!-- done -->
        <style token="LITERAL_BACKQUOTE" fg="A5C25C"/><!-- done -->
        <style token="MARKUP_TAG_DELIMITER" fg="678CB1"/>
        <style token="MARKUP_TAG_NAME" fg="9876AA" bold="true"/>
        <style token="MARKUP_TAG_ATTRIBUTE" fg="B3B689"/>
        <style token="MARKUP_TAG_ATTRIBUTE_VALUE" fg="e1e2cf"/>
        <style token="MARKUP_COMMENT" fg="66747B"/>
        <style token="MARKUP_DTD" fg="A082BD"/>
        <style token="MARKUP_PROCESSING_INSTRUCTION" fg="A082BD"/>
        <style token="MARKUP_CDATA" fg="d5e6f0"/>
        <style token="MARKUP_CDATA_DELIMITER" fg="ae9fbf"/>
        <style token="MARKUP_ENTITY_REFERENCE" fg="678CB1"/>
        <style token="OPERATOR" fg="A9B7C6"/><!-- done -->
        <style token="PREPROCESSOR" fg="A082BD"/>
        <style token="REGEX" fg="d39745"/>
        <style token="SEPARATOR" fg="E8E2B7"/>
        <style token="VARIABLE" fg="7E7E7E" bold="true"/>
        <style token="WHITESPACE" fg="E0E2E4"/>

        <style token="ERROR_IDENTIFIER" fg="E0E2E4" bg="04790e"/>
        <style token="ERROR_NUMBER_FORMAT" fg="E0E2E4" bg="04790e"/>
        <style token="ERROR_STRING_DOUBLE" fg="E0E2E4" bg="04790e"/>
        <style token="ERROR_CHAR" fg="E0E2E4" bg="04790e"/>
    </tokenStyles>

</RSyntaxTheme>
